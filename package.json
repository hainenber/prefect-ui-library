{
    "name": "@prefecthq/prefect-ui-library",
    "version": "2.0.12",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/PrefectHQ/prefect-ui-library"
    },
    "bugs": {
        "url": "https://github.com/PrefectHQ/prefect-ui-library/issues"
    },
    "homepage": "https://github.com/PrefectHQ/prefect-ui-library#readme",
    "keywords": [
        "prefect 2.0",
        "vue3"
    ],
    "scripts": {
        "build:demo": "vite build --mode=demo",
        "build": "vue-tsc && tsc-alias && vite build",
        "changelog": "auto-changelog --package --commit-limit 0",
        "dev": "vue-tsc -w & tsc-alias -w & vite build -w --force",
        "lint:fix": "eslint src --fix",
        "lint": "eslint src",
        "serve": "vite --mode=demo --host",
        "test": "vitest",
        "validate:types": "vue-tsc --noEmit",
        "version": "npm run changelog && git add CHANGELOG.md"
    },
    "main": "./dist/prefect-ui-library.umd.js",
    "module": "./dist/prefect-ui-library.mjs",
    "exports": {
        ".": {
            "import": "./dist/prefect-ui-library.mjs",
            "require": "./dist/prefect-ui-library.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/src/index.d.ts",
    "devDependencies": {
        "@prefecthq/eslint-config": "1.0.31",
        "@types/d3": "7.4.0",
        "@types/lodash.camelcase": "4.3.7",
        "@types/lodash.debounce": "4.0.7",
        "@types/lodash.merge": "4.6.7",
        "@types/node": "^20.6.2",
        "@types/prismjs": "^1.26.0",
        "@vitejs/plugin-vue": "4.3.4",
        "auto-changelog": "^2.4.0",
        "autoprefixer": "10.4.15",
        "eslint": "8.49.0",
        "lodash.camelcase": "4.3.0",
        "lodash.debounce": "4.0.8",
        "postcss": "8.4.30",
        "tailwindcss": "3.3.3",
        "tsc-alias": "1.8.7",
        "typescript": "5.2.2",
        "vite": "4.4.9",
        "vite-svg-loader": "^4.0.0",
        "vitest": "^0.34.4",
        "vue-tsc": "1.8.11"
    },
    "peerDependencies": {
        "@prefecthq/prefect-design": "^2.0.8",
        "@prefecthq/vue-charts": "^2.0.2",
        "@prefecthq/vue-compositions": "^1.5.9",
        "vee-validate": "^4.7.0",
        "vue": "^3.3.4",
        "vue-router": "^4.0.12"
    },
    "dependencies": {
        "@prefecthq/graphs": "1.1.0",
        "@types/lodash.isequal": "4.5.6",
        "axios": "0.27.2",
        "cronstrue": "^2.32.0",
        "d3": "7.8.5",
        "date-fns": "2.30.0",
        "date-fns-tz": "1.3.7",
        "lodash.isequal": "4.5.0",
        "lodash.merge": "4.6.2",
        "prismjs": "1.29.0"
    }
}
