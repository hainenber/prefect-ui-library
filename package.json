{
    "name": "@prefecthq/orion-design",
    "version": "1.0.44",
    "private": false,
    "keywords": [
        "prefect 2.0",
        "vue3"
    ],
    "scripts": {
        "build:demo": "vite build --mode=demo",
        "build": "vue-tsc && tsc-alias && vite build",
        "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "serve": "vite --mode=demo --host"
    },
    "main": "./dist/orion-design.umd.js",
    "module": "./dist/orion-design.es.js",
    "exports": {
        ".": {
            "import": "./dist/orion-design.es.js",
            "require": "./dist/orion-design.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/src/index.d.ts",
    "devDependencies": {
        "@prefecthq/eslint-config": "1.0.16",
        "@tailwindcss/line-clamp": "0.4.0",
        "@types/lodash.debounce": "4.0.7",
        "@types/node": "^18.0.6",
        "@vitejs/plugin-vue": "2.3.3",
        "autoprefixer": "10.4.5",
        "eslint": "8.20.0",
        "lodash.debounce": "4.0.8",
        "postcss": "8.4.14",
        "tailwindcss": "3.1.6",
        "tsc-alias": "1.6.11",
        "typescript": "^4.7.4",
        "vite": "2.9.13",
        "vue-tsc": "0.38.8"
    },
    "peerDependencies": {
        "@prefecthq/prefect-design": "^1.0.28",
        "pinia": "^2.0.14",
        "vee-validate": "^4.5.11",
        "vue": "^3.2.34",
        "vue-router": "^4.0.12"
    },
    "dependencies": {
        "@prefecthq/radar": "0.0.16",
        "@prefecthq/vue-charts": "0.0.15",
        "@prefecthq/vue-compositions": "0.1.38",
        "axios": "0.27.2",
        "cronstrue": "^2.11.0",
        "date-fns": "2.28.0",
        "date-fns-tz": "1.3.6",
        "rrule": "^2.7.1"
    }
}
