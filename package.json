{
    "name": "@prefecthq/orion-design",
    "version": "1.0.85",
    "private": false,
    "keywords": [
        "prefect 2.0",
        "vue3"
    ],
    "scripts": {
        "build:demo": "vite build --mode=demo",
        "build": "vue-tsc && tsc-alias && vite build",
        "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "serve": "vite --mode=demo --host"
    },
    "main": "./dist/orion-design.umd.js",
    "module": "./dist/orion-design.es.js",
    "exports": {
        ".": {
            "import": "./dist/orion-design.es.js",
            "require": "./dist/orion-design.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/src/index.d.ts",
    "devDependencies": {
        "@prefecthq/eslint-config": "1.0.16",
        "@tailwindcss/line-clamp": "0.4.0",
        "@types/lodash.debounce": "4.0.7",
        "@types/node": "^18.7.14",
        "@types/prismjs": "^1.26.0",
        "@vitejs/plugin-vue": "2.3.3",
        "autoprefixer": "10.4.8",
        "eslint": "8.23.0",
        "lodash.debounce": "4.0.8",
        "postcss": "8.4.16",
        "tailwindcss": "3.1.8",
        "tsc-alias": "1.7.0",
        "typescript": "^4.7.4",
        "vite": "2.9.13",
        "vue-tsc": "0.40.4"
    },
    "peerDependencies": {
        "@prefecthq/prefect-design": "1.1.3",
        "pinia": "^2.0.14",
        "vee-validate": "^4.5.11",
        "vue": "^3.2.34",
        "vue-router": "^4.0.12"
    },
    "dependencies": {
        "@prefecthq/radar": "0.0.17",
        "@prefecthq/vue-charts": "0.0.19",
        "@prefecthq/vue-compositions": "0.1.43",
        "axios": "0.27.2",
        "cronstrue": "^2.11.0",
        "date-fns": "2.29.2",
        "date-fns-tz": "1.3.6",
        "prismjs": "1.28.0",
        "rrule": "^2.7.1"
    }
}
