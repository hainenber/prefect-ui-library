{
    "name": "@prefecthq/orion-design",
    "version": "1.1.26",
    "private": false,
    "keywords": [
        "prefect 2.0",
        "vue3"
    ],
    "scripts": {
        "build:demo": "vite build --mode=demo",
        "build": "vue-tsc && tsc-alias && vite build",
        "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "serve": "vite --mode=demo --host",
        "changelog": "auto-changelog --package --commit-limit 0",
        "changelog:commit": "npm run changelog && git add -A && git commit -m 'Update CHANGELOG.md'",
        "release:patch": "npm version patch && npm run changelog:commit",
        "release:minor": "npm version minor && npm run changelog:commit",
        "release:major": "npm version major && npm run changelog:commit'"
    },
    "main": "./dist/orion-design.umd.js",
    "module": "./dist/orion-design.mjs",
    "exports": {
        ".": {
            "import": "./dist/orion-design.mjs",
            "require": "./dist/orion-design.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/src/index.d.ts",
    "devDependencies": {
        "@prefecthq/eslint-config": "1.0.17",
        "@tailwindcss/line-clamp": "0.4.2",
        "@types/lodash.camelcase": "4.3.7",
        "@types/lodash.debounce": "4.0.7",
        "@types/node": "^18.11.9",
        "@types/prismjs": "^1.26.0",
        "@vitejs/plugin-vue": "3.2.0",
        "auto-changelog": "^2.4.0",
        "autoprefixer": "10.4.13",
        "eslint": "8.28.0",
        "lodash.camelcase": "4.3.0",
        "@types/marked": "^4.0.5", 
        "@types/sanitize-html": "^2.6.2",
        "lodash.debounce": "4.0.8",
        "postcss": "8.4.19",
        "tailwindcss": "3.2.4",
        "tsc-alias": "1.7.1",
        "typescript": "^4.9.3",
        "vite": "3.2.4",
        "vite-svg-loader": "^3.6.0",
        "vue-tsc": "1.0.9"
    },
    "peerDependencies": {
        "@prefecthq/prefect-design": "^1.1.33",
        "vee-validate": "^4.5.11",
        "vue": "^3.2.45",
        "vue-router": "^4.0.12"
    },
    "dependencies": {
        "@fontsource/inconsolata": "^4.5.9",
        "@fontsource/inter": "4.5.14",
        "@prefecthq/radar": "0.0.17",
        "@prefecthq/vue-charts": "0.0.19",
        "@prefecthq/vue-compositions": "1.0.0",
        "axios": "0.27.2",
        "cronstrue": "^2.19.0",
        "date-fns": "2.29.3",
        "date-fns-tz": "1.3.7",
        "prismjs": "1.29.0",
        "marked": "4.0.18",
        "sanitize-html": "^2.7.1"
    }
}
